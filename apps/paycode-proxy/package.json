{
  "name": "@paycode-customer-v2/paycode-proxy",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build-core",
    "build-balance": "cd fn/balance && npm run build",
    "build-payment": "cd fn/payment && npm run build",
    "build-transaction": "cd fn/transaction && npm run build",
    "bundle-api": "node scripts/bundle-openapi.js",
    "build-template": "node scripts/build-template.js",
    "build-api": "npm run bundle-api && npm run validate-api && npm run build-template && npm run validate-template",
    "build-core": "concurrently \"npm:build-api\" \"npm:build-payment\" \"npm:build-balance\" \"npm:build-transaction\"",
    "deploy": "node scripts/deploy.js",
    "deploy-api": "node scripts/deploy-api.js",
    "patch-api": "node scripts/patch-api.js",
    "postbuild": "npm run deploy && npm run patch-api && npm run deploy-api",
    "prebuild": "npm install && tsc && rimraf samconfig.toml template.yaml",
    "test": "echo \"Error: no test specified\" && exit 1",
    "validate-template": "sam validate --lint",
    "validate-api": "node scripts/validate-openapi.js",
    "watch": "tsc -w"
  },
  "author": "",
  "license": "ISC",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.x",
    "@aws-sdk/client-api-gateway": "^3.x",
    "@aws-sdk/credential-providers": "3.*",
    "@paycode-customer-v2/config": "*",
    "@paycode-customer-v2/lib": "*",
    "@paycode-customer-v2/utils": "*",
    "@redocly/cli": "^1.x",
    "@redocly/openapi-core": "^1.x",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "axios": "^1.x",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "rimraf": "5.x",
    "ts-jest": "^29.1.2",
    "concurrently": "^8.x",
    "change-case-all": "^2.x",
    "cross-env": "^7.x",
    "fs-extra": "^11.x",
    "orval": "^6.x",
    "yaml": "^2.3.4"
  },
  "description": "",
  "dependencies": {}
}

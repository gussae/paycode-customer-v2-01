{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "rootDir": "./src",
    "baseUrl": "./",
    "module": "ESNext",
    "outDir": "./dist",
    "composite": true,
    "noEmit": false
  },
  "exclude": ["node_modules", "build", "tsconfig.json"],
  "include": [
    "src/**/*.ts",
    "src/**/*.js",
    "src/**/*.json",
    "src/.generated/**/*.ts"
  ]
}
